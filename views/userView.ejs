<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>WELCOME</h1>
    <h3><%=dataUser.name%></h3>
    <h1>MAKAN BARENG</h1>
    <a href="/thread/joinThread/<%=dataUser.id%>">Gabung Acara Makan Bareng</a>

    <a href="/thread/user/add">Buat Acara Makan Bareng</a>
    <table border="1">
        <thead>
          <tr>
            <th>id</th>
            <th>Acara Party</th>
            <th>Tema Makanan</th>
            <th>Makanan yang kamu bawa</th>
            <th>Time Started</th>
          </tr>
        </thead>
        <tbody>

          <%dataThreads.forEach(function(data){%>
          <tr>

              <td><%= data.id %></td>
              <td><%= data.judulThread %></td>
              <td><%= data.JenisMakananID %></td>
              <td><%=data.Makanan.namaMakanan%></td>
              <td><%= data.waktuMulai %></td>
            <%})%>
          </tr>
        </tbody>
    </body>
  </html>
