<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>List Acara Makan Bareng</h1>
    <table border="1">
        <thead>
          <tr>
            <th>id</th>
            <th>judulThread</th>
            <th>jenisMakananid</th>
            <th>waktuMulai</th>
          </tr>
        </thead>
        <tbody>
          <%keyThread.forEach(data =>{%>
                    <td><%=data.id%></td>
                    <td><%=data.judulThread%></td>
                    <td><%=data.JenisMakananID%></td>
                    <td><%=data.waktuMulai%></td>
                    <td><a href="/thread/join/<%=data.id%>">join</td>
                </tr>
          <%})%>
        </tbody>
    </body>
  </html>
