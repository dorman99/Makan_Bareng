<!DOCTYPE html>
<html>
  <head>
    <%- include ./partials/materilzeinit%>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>THREAD LIST</h1>
    <a href="/thread/add">ADD THREAD</a>
    <div class="container">
        <table class="highlight">
        <thead>
          <tr>
            <th>id</th>
            <th>judulThread</th>
            <th>jenisMakananid</th>
            <th>waktuMulai</th>
            <th>waktuBerakhir</th>
            <th>edit</th>
            <th>delete</th>
          </tr>
        </thead>
        <tbody>
          <%keyThread.forEach(data =>{%>
          <tr>
              <td><%=data.id%></td>
              <td><%=data.judulThread%></td>
              <td><%=data.JenisMakananID%></td>
              <td><%=data.waktuMulai%></td>
              <td><%=data.waktuBerakhir%></td>
              <td><a href="/thread/edit/<%=data.id%>">edit</td>
              <td><a href="/thread/delete/<%=data.id%>">delete</td>
          </tr>
          <%})%>
        </tbody>
      </table>
    </div>
    </body>
  </html>
