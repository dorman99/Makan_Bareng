<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>ITEMS</h1>
    <a href="/thread/add">ADD THREAD</a>
    <table border="1">
        <thead>
          <tr>
            <th>id</th>
            <th>judulThread</th>
            <th>jenisMakananid</th>
            <th>waktuMulai</th>
            <th>waktuBerakhir</th>
          </tr>
        </thead>
        <tbody>
          <%keyThread.forEach(data =>{%>
          <tr>
              <td><%=data.id%></td>
              <td><%=data.judulThread%></td>
              <td><%=data.JenisMakananID%></td>
              <td><%=data.waktuMulai%></td>
              <td><%=data.waktuBerakhir%></td>
              <td><a href="/thread/edit/<%=data.id%>">edit</td>
              <td><a href="/thread/delete/<%=data.id%>" action="/items/delete/:id" method="get">delete</td>
          </tr>
          <%})%>
        </tbody>
    </body>
  </html>
